generateMuestraGraphs<-function(n.rep,n){
doy = seq(from=1, to=365, by=1)
sim.mat = matrix(NA, nrow=n, ncol=2)
for(i in 2:n){
bday = replicate(n.rep, sample(doy, size=i, replace=T) )
}
return(bday)
}
