##funciones que utilizare en shiny App
#-----------------------------------------------------
#generatedata:: función que se emplea en la página 1 del dashboard.
# input:: numero de personas que se quiere simular su cumpleaños
# output:: dataframe de 3 columnas: nombre, cumpleaños, numero de fila
generatedata<-function(n){
Birthday<-sample(seq(as.Date('2018/01/01'), as.Date('2018/12/31'), by="day"), n, replace=TRUE)
muestra<-sample(nrow(Name),n)
Name<-Name[muestra,1]
Name2<-seq(1:n)
data<-data.frame(Name, Birthday,Name2)
return(data)
}
#===========================================================
#generatecoincidences:: función que se emplea en la página 1 del dashboard.
# input:: dataframe generado por la funcion generatedata
# output:: dataframe de 3 columnas: nombre, cumpleaños, numero de fila de las personas que coincide su cumpleaños
generatecoincidences<-function(matriz){
cumple<-matriz[duplicated(matriz$Birthday),]
cumple<-cumple$Birthday
repetidos<-data.frame(Name="No existen coincidencias",Birthday="")
repetidos2<-data.frame()
if (length(cumple)!=0) {
for (i in 1:length(cumple)){
cumple2<-matriz[(matriz$Birthday==cumple[i]),]
repetidos2<-rbind(repetidos2,cumple2)
}
repetidos<-repetidos2
}
colnames(repetidos) <- c("Name", "Same Birthday")
return(repetidos)
}
#==============================================================
#generategraphs:: función que se emplea en la página 2 del dashboard.
# input:: dataframe generado por el numero de personas y simulaciones que se introducen, t=número de cumpleaños coincidentes
# output:: probabilidades de la simulación para el número de dias coincidentes t
generategraphs<-function(bday,t){
n<-nrow(bday)
sim.mat = matrix(NA, nrow=n, ncol=2)
for(i in 2:n){
#print(bday[seq(1,i),])
bday1=bday[seq(1,i),]
bday.table = apply(bday1, 2, table)
sim.2 = ifelse(unlist(lapply(bday.table, max) ) >=t, 1, 0)
sim.mat[i,1] = i
sim.mat[i,2] = sum(sim.2)/length(sim.2)
}
graph.sim = t( sim.mat[,2] )
colnames(graph.sim) = sim.mat[,1]
return(graph.sim)
}
#=========================================================
#graphconsecutiveD:: función que se emplea en la página 2 del dashboard.
# input:: dataframe generado por el numero de personas y simulaciones que se introducen
# output:: probabilidades de la simulación para  dias consecutivos
graphconsecutiveD<-function(bday){
n<-nrow(bday)
sim.mat = matrix(NA, nrow=n, ncol=2)
for(i in 2:n){
bday1=bday[seq(1,i),]
bday.table=apply(bday1,2,unique)
bday.table=apply(bday1,2,sort)
bday.table1=apply(bday.table,2,function(x){c(0,diff(x))}) #primeras diferencias:cuento num 1
bday.table1[bday.table1!=1]=0
sim.2<-apply(bday.table1,2,max)
sim.mat[i,1] = i
sim.mat[i,2] = sum(sim.2)/ncol(bday)
}
graph.sim = t( sim.mat[,2] )
colnames(graph.sim) = sim.mat[,1]
return(graph.sim)
}
#==============================================================
#generateMuestraGraphs:: función que se emplea en la página 2 del dashboard.
# input:: numero de simulaciones y numero de personas
# output:: matriz con la muestra de simulaciones
generateMuestraGraphs<-function(n.rep,n){
doy = seq(from=1, to=365, by=1)
sim.mat = matrix(NA, nrow=n, ncol=2)
for(i in 2:n){
bday = replicate(n.rep, sample(doy, size=i, replace=T) )
}
return(bday)
}
